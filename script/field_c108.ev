//--------------------------------------------------------------------
#define LOCKED_DOOR_CELL_CODE		584
#define LOCKED_DOOR_GYM_CELL_CODE	592

bg_change_field_c108:
	//不思議な洞窟終了してない
	_IF_FLAGOFF_CALL	FE_POKE1_01_CAVE_D1111,door_cell_chg_01_field_c108

	//不思議な洞窟終了した
	_IF_FLAGON_CALL		FE_POKE1_01_CAVE_D1111,door_cell_chg_02_field_c108
	_END

//不思議な洞窟終了してない
door_cell_chg_01_field_c108:
	_SXY_POS_CHANGE		OLDMAN1_01_FIELD_C108,31,18
	_SET_CELL			31,32,LOCKED_DOOR_GYM_CELL_CODE,1		//ポケモンジム
	_RET

//不思議な洞窟終了した
door_cell_chg_02_field_c108:
	//超古代ポケモンイベント終了まで町のドアを閉めておく
	_IF_FLAGOFF_CALL	FE_POKE1_01_CAVE_D1206,door_cell_chg_field_c108
	_RET

//超古代ポケモンイベント終了まで町のドアを閉めておく
door_cell_chg_field_c108:
	_SET_CELL			9,6,LOCKED_DOOR_CELL_CODE,1
	_SET_CELL			9,17,LOCKED_DOOR_CELL_CODE,1
	_SET_CELL			9,26,LOCKED_DOOR_CELL_CODE,1
	_SET_CELL			44,17,LOCKED_DOOR_CELL_CODE,1
	_SET_CELL			8,35,LOCKED_DOOR_CELL_CODE,1
	_SET_CELL			53,28,LOCKED_DOOR_CELL_CODE,1
	_SET_CELL			45,6,LOCKED_DOOR_CELL_CODE,1
	_SET_CELL			48,25,LOCKED_DOOR_CELL_CODE,1
	_SET_CELL			31,32,LOCKED_DOOR_GYM_CELL_CODE,1		//ポケモンジム
	_RET


//--------------------------------------------------------------------
//						FLAG_CHANGE_LABEL
//--------------------------------------------------------------------
//到着フラグセット
ev_arrive_flag_set_c108:
	_FLAG_SET		SYS_C108_ARRIVE

	//超古代ポケモンイベント終了している
	_IF_FLAGON_CALL	FE_POKE1_01_CAVE_D1206,pos_chg_daigo_01_p01_field_c108

	//大雨中だったらトレーナーの位置をずらす
	_IF_FLAGON_CALL		SYS_WEATHER_CTRL,pos_chg_trainer_field_c108
	_END

//大雨中だったらトレーナーの位置をずらす
pos_chg_trainer_field_c108:
	//大雨
	_CALL			ctrl_weather_thunder

	_SXY_POS_CHANGE		WORKERW2_01_FIELD_C108,29,19
	_SXY_POS_CHANGE		WORKERW2_02_FIELD_C108,33,19

	//まだ案内してなかったら
	_IFVAL_CALL		WK_SCENE_FIELD_C108,EQ,1,pos_chg_mikuri_01_p02_field_c108
	_RET

//ミクリを洞窟の上の方に移動させる
pos_chg_mikuri_01_p02_field_c108:
	_SXY_POS_CHANGE		DAIGO_01_FIELD_C108,29,6
	_SXY_SITE_CHANGE	DAIGO_01_FIELD_C108,MV_RIGHT
	_SXY_POS_CHANGE		MIKURI_01_FIELD_C108,30,6
	_SXY_SITE_CHANGE	MIKURI_01_FIELD_C108,MV_LEFT
	_RET

//超古代ポケモンイベント終了していたら、ダイゴをジムの前に移動する
pos_chg_daigo_01_p01_field_c108:
	_SXY_POS_CHANGE		DAIGO_01_FIELD_C108,31,33
	_SXY_SITE_CHANGE	DAIGO_01_FIELD_C108,MV_DOWN
	_RET

