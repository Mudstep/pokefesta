/*								POS									*/
/********************************************************************/
SCRIPT_LABEL	ev_field_c108_pos01
	_TALK_START

	//振り向く
	_SET_OBJ_ANIME	DAIGO_01_FIELD_C108,turn_l_anime
	_SET_OBJ_ANIME	EV_OBJ_SPID,turn_r_anime
	_OBJ_ANIME_WAIT				//アニメ終了待ち
	
	_SE				SE_GYOE_MARK
	_SET_OBJ_ANIME	DAIGO_01_FIELD_C108,gyoe_mark_anime
	_OBJ_ANIME_WAIT				//アニメ終了待ち

	_SET_OBJ_ANIME	DAIGO_01_FIELD_C108,gyoe_mark_wait_anime
	_OBJ_ANIME_WAIT				//アニメ終了待ち

	//近づいてくる
	_SET_OBJ_ANIME	DAIGO_01_FIELD_C108,daigo_01_field_c108_anime01
	_OBJ_ANIME_WAIT				//アニメ終了待ち

#if ( PM_VERSION == VERSION_SAPPHIRE )
	_TALK_KEYWAIT	msg_daigo_01_p01_field_c108s
#elif ( PM_VERSION == VERSION_RUBY )
	_TALK_KEYWAIT	msg_daigo_01_p01_field_c108r
#endif

	_TALK_CLOSE

	//連れ歩き
	_SET_OBJ_ANIME	DAIGO_01_FIELD_C108,daigo_01_field_c108_anime02
	_SET_OBJ_ANIME	EV_OBJ_SPID,hero_01_field_c108_anime01
	_OBJ_ANIME_WAIT DAIGO_01_FIELD_C108				//アニメ終了待ち

#if ( PM_VERSION == VERSION_SAPPHIRE )
	_TALK_KEYWAIT	msg_mikuri_01_p01_field_c108s
#elif ( PM_VERSION == VERSION_RUBY )
	_TALK_KEYWAIT	msg_mikuri_01_p01_field_c108r
#endif

	_SE				SE_GYOE_MARK
	_SET_OBJ_ANIME	MIKURI_01_FIELD_C108,gyoe_mark_anime
	_OBJ_ANIME_WAIT				//アニメ終了待ち

	_SET_OBJ_ANIME	MIKURI_01_FIELD_C108,gyoe_mark_wait_anime
	_OBJ_ANIME_WAIT				//アニメ終了待ち

#if ( PM_VERSION == VERSION_SAPPHIRE )
	_TALK_KEYWAIT	msg_mikuri_01_p02_field_c108s
#elif ( PM_VERSION == VERSION_RUBY )
	_TALK_KEYWAIT	msg_mikuri_01_p02_field_c108r
#endif

	_TALK_CLOSE

	_TIME_WAIT		30

	//連れ歩き
	_SET_OBJ_ANIME	MIKURI_01_FIELD_C108,mikuri_01_field_c108_anime01
	_SET_OBJ_ANIME	DAIGO_01_FIELD_C108,daigo_01_field_c108_anime03
	_SET_OBJ_ANIME	EV_OBJ_SPID,hero_01_field_c108_anime02
	_OBJ_ANIME_WAIT

	//上を向く
	_SET_OBJ_ANIME	MIKURI_01_FIELD_C108,turn_r_anime
	_SET_OBJ_ANIME	EV_OBJ_SPID,turn_r_anime
	_OBJ_ANIME_WAIT

	_TIME_WAIT		30

	//下を向く
	_SET_OBJ_ANIME	DAIGO_01_FIELD_C108,turn_d_anime
	_OBJ_ANIME_WAIT

#if ( PM_VERSION == VERSION_SAPPHIRE )
	_TALK_KEYWAIT	msg_daigo_01_p02_field_c108s
#elif ( PM_VERSION == VERSION_RUBY )
	_TALK_KEYWAIT	msg_daigo_01_p02_field_c108r
#endif

	_TALK_CLOSE

	_TIME_WAIT		30

	//連れ歩き
	_SET_OBJ_ANIME	MIKURI_01_FIELD_C108,mikuri_01_field_c108_anime02
	_SET_OBJ_ANIME	EV_OBJ_SPID,hero_01_field_c108_anime03
	_SET_OBJ_ANIME	DAIGO_01_FIELD_C108,daigo_01_field_c108_anime04
	_OBJ_ANIME_WAIT

	//左を向く
	_SET_OBJ_ANIME	MIKURI_01_FIELD_C108,turn_l_anime
	_OBJ_ANIME_WAIT

#if ( PM_VERSION == VERSION_SAPPHIRE )
	_TALK_KEYWAIT	msg_mikuri_01_p03_field_c108s
#elif ( PM_VERSION == VERSION_RUBY )
	_TALK_KEYWAIT	msg_mikuri_01_p03_field_c108r
#endif

	//左を向く
	_SET_OBJ_ANIME	EV_OBJ_SPID,turn_l_anime
	_OBJ_ANIME_WAIT

#if ( PM_VERSION == VERSION_SAPPHIRE )
	_TALK_KEYWAIT	msg_daigo_01_p03_field_c108s
#elif ( PM_VERSION == VERSION_RUBY )
	_TALK_KEYWAIT	msg_daigo_01_p03_field_c108r
#endif

	//洞窟に入らないで、スクロールしてまた表示させるとまずいので
	_SXY_POS_OVERWRITE		MIKURI_01_FIELD_C108
	_SXY_SITE_CHANGE		MIKURI_01_FIELD_C108,MV_LEFT
	_SXY_POS_OVERWRITE		DAIGO_01_FIELD_C108
	_SXY_SITE_CHANGE		DAIGO_01_FIELD_C108,MV_RIGHT

	_LDVAL			WK_SCENE_FIELD_C108,2
	_TALK_END
	_END

