/********************************************************************/
SCRIPT_LABEL	ev_girl2_01_p01_field_c108
	_TALK_OBJ_START

	_UPDATE_GAME_TIME
	_PROGRAM		SET_SEX_NAME_01

	//一日一回
	_IF_FLAGON_JUMP	TMFLG_SEED_PRESENT_09,ev_girl2_01_p02_field_c108

	_TALK_KEYWAIT	msg_girl2_01_p01_kinomi

	_GET_RND		10
	//_ADDWK			ANSWORK,21
	_ADDWK			ANSWORK,20
	_ADDWK			ANSWORK,ITEM_SEED_START
	_ITEM_EVENT		ANSWORK,1
	_IF_ZERO_JUMP	ev_fail_item

	_FLAG_SET		TMFLG_SEED_PRESENT_09

	_TALK_KEYWAIT	msg_girl2_01_p02_kinomi
	_GET_RND		2
	_IFVAL_JUMP		ANSWORK,EQ,0,case0_girl2_01_kinomi
	_IFVAL_JUMP		ANSWORK,EQ,1,case1_girl2_01_kinomi
	_END

//ランダムでどちらか貰える
case0_girl2_01_kinomi:
	_ITEM_EVENT		ITEM_FIRANOMI,1
	_IF_ZERO_JUMP	ev_fail_item
	_TALK_KEYWAIT	msg_girl2_01_p03_kinomi
	_TALK_OBJ_END
	_END

case1_girl2_01_kinomi:
	_ITEM_EVENT		ITEM_IANOMI,1
	_IF_ZERO_JUMP	ev_fail_item
	_TALK_KEYWAIT	msg_girl2_01_p03_kinomi
	_TALK_OBJ_END
	_END

//一日一回
ev_girl2_01_p02_field_c108:
	_TALK_YES_NO	msg_girl2_01_p04_kinomi
	_IFVAL_JUMP		ANSWORK,EQ,TRUE,yes_girl2_01_p01_field_c108

	_TALK_KEYWAIT	msg_girl2_01_p06_kinomi
	_TALK_OBJ_END
	_END

//「はい」
yes_girl2_01_p01_field_c108:
	_TALK_KEYWAIT	msg_girl2_01_p05_kinomi
	_TALK_OBJ_END
	_END


