/********************************************************************/
/*							母親									*/
//
//		部屋を出るまで有効用にLOCALWORK1を使用しているので注意
/********************************************************************/
SCRIPT_LABEL	ev_opening_mama_talk
	_TALK_OBJ_START

	//初めてのゲームクリアからフィールドに出るまで
	_IFVAL_JUMP		WK_SCENE_T101_R0101,EQ,4,ev_mama_hunenotikette
	_IFVAL_JUMP		WK_SCENE_T101_R0201,EQ,4,ev_mama_hunenotikette

	//トウカのジムバッジ入手後か
	_IF_FLAGON_JUMP		SYS_BATCH05_GET,ev_mama_item_get_01

	//博士からポケモンをもらった後
	_IF_FLAGON_JUMP		FE_ODAMAKI_01_P01_T101_R0301,ev_woman1_01_get_ball_after

	//まだ一度も外へ出ていない
	_IFVAL_JUMP		LOCALWORK1,EQ,1,ev_not_exit

	//自宅イベントが終了した
	_IFVAL_JUMP		WK_SCENE_SP_OPENING,GE,7,ev_opening_end

	_TALK_KEYWAIT	msg_mama_01_p01_t101_r0101
	_TALK_OBJ_END
	_END

//初めてのゲームクリアからフィールドに出るまで
ev_mama_hunenotikette:
	_TALK_KEYWAIT	msg_mama_01_p14_t101_r0101
	_TALK_OBJ_END
	_END

//トウカのジムバッジ入手後
ev_mama_item_get_01:
	_IF_FLAGON_JUMP	FE_OMAMORIKOBAN_GET,ev_woman1_01_get_ball_after

	_TALK_KEYWAIT	msg_mama_01_p11_t101_r0101

	_ITEM_EVENT		ITEM_OMAMORIKOBAN,1
	_IF_ZERO_JUMP	ev_fail_item

	_TALK_KEYWAIT	msg_mama_01_p13_t101_r0101
	_FLAG_SET		FE_OMAMORIKOBAN_GET
	_TALK_OBJ_END
	_END

//博士からポケモンをもらった後
ev_woman1_01_get_ball_after:
	_TALK_KEYWAIT	msg_mama_01_p09_t101_r0101
	_JUMP			ev_mama_normal_kaihuku
	_END

//まだ一度も外へ出ていない
ev_not_exit:
	_TALK_KEYWAIT	msg_mama_01_p15_t101_r0101
	_TALK_OBJ_END
	_END

//自宅で回復
ev_mama_normal_kaihuku:
	_TALK_CLOSE
	_CALL			normal_kaifuku
	_INC_RECORD		CNT_KAIFUKU_HOME	//自宅で回復した回数をカウント

	_TALK_KEYWAIT	msg_mama_01_p10_t101_r0101
	_TALK_OBJ_END
	_END

//自宅イベントが終了した
SCRIPT_LABEL	ev_opening_end
	_TALK_KEYWAIT	msg_mama_01_p08_t101_r0101
	_TALK_OBJ_END
	_END


