/********************************************************************/
//						時計を合わせる
/********************************************************************/
//t101_r0102
SCRIPT_LABEL	ev_opening_clock_set_01
	_TALK_START
	_LDVAL		PARAM0,0			//時計画面で使用する
	_JUMP		ev_common_clock
	_END

//t101_r0202
SCRIPT_LABEL	ev_opening_clock_set_02
	_TALK_START
	_LDVAL		PARAM0,1			//時計画面で使用する
	_JUMP		ev_common_clock
	_END

//共通処理
ev_common_clock:
	//時刻設定をしているか
	_IF_FLAGON_JUMP	FE_CLOCK_SET,already_clock_set

    _TALK_KEYWAIT	msg_tokei1_01_p01_t101_r0102

	//時刻設定画面へ
	_CALL			ev_clock_set

	_TIME_WAIT		30

	_LDVAL			WK_SCENE_SP_OPENING,6
	_FLAG_SET		FE_CLOCK_SET
	_FLAG_SET		FV_POKE1_01_OPENING_ROOM01
	_FLAG_SET		FV_POKE1_02_OPENING_ROOM01

	//男女の性別を判断する
	_GET_MY_SEX
	_IFVAL_CALL		ANSWORK,EQ,0,my_sex_boy_sp_opening
	_IFVAL_CALL		ANSWORK,EQ,1,my_sex_girl_sp_opening

	_SE				SE_KAIDAN

	//母を消す
	_OBJ_DELETE		TMPWK0

	_TALK_END
	_END

//主人公(男)
my_sex_boy_sp_opening:
	_LDVAL			TMPWK0,MAMA_01_T101_R0102

	//母を出す
	_OBJ_APPEAR		TMPWK0

	//近づいてくる
	_SET_OBJ_ANIME	TMPWK0,mama_01_sp_opening_anime50
	_OBJ_ANIME_WAIT

	//母の方を向く
	_SET_OBJ_ANIME	EV_OBJ_SPID,turn_r_anime
	_OBJ_ANIME_WAIT
	
	_TALK_KEYWAIT	msg_mama_01_p01_t101_r0102
	_TALK_CLOSE

	//去っていく
	_SET_OBJ_ANIME	TMPWK0,mama_01_sp_opening_anime51
	_OBJ_ANIME_WAIT
	_RET

//主人公(女)
my_sex_girl_sp_opening:
	_LDVAL			TMPWK0,MAMA_01_T101_R0202

	//母を出す
	_OBJ_APPEAR		TMPWK0

	//近づいてくる
	_SET_OBJ_ANIME	TMPWK0,mama_01_sp_opening_anime52
	_OBJ_ANIME_WAIT

	//母の方を向く
	_SET_OBJ_ANIME	EV_OBJ_SPID,turn_l_anime
	_OBJ_ANIME_WAIT

	_TALK_KEYWAIT	msg_mama_01_p01_t101_r0102
	_TALK_CLOSE

	//去っていく
	_SET_OBJ_ANIME	TMPWK0,mama_01_sp_opening_anime53
	_OBJ_ANIME_WAIT
	_RET

//時刻設定をしている
already_clock_set:
	//記録カウントアップ
