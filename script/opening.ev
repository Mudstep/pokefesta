	//主人公の方を向く
	_SET_OBJ_ANIME	PARAM0,turn_hero_site_anime
	_OBJ_ANIME_WAIT

	//男女で分岐
	_IFVAL_CALL		PARAM1,EQ,0,case0_opening_hero_room01
	_IFVAL_CALL		PARAM1,EQ,1,case1_opening_hero_room01

	_TALK_KEYWAIT	msg_mama_01_p02_t101_r0101
	_TALK_CLOSE
	
	_LDVAL			WK_SCENE_SP_OPENING,4

	//上に移動する
	_SET_OBJ_ANIME	EV_OBJ_SPID,hero_01_opening_anime10
	_SET_OBJ_ANIME	PARAM0,turn_u_anime
	_OBJ_ANIME_WAIT

	_TALK_END
	_END

