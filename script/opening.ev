/********************************************************************/
/*					パパの「船のチケット」イベント					*/
/********************************************************************/
SCRIPT_LABEL	ev_papa_hunenotiketto
	_TALK_START

	//男女の性別を判断する
	_GET_MY_SEX
	_IFVAL_CALL		ANSWORK,EQ,0,my_sex_boy_papa_hune
	_IFVAL_CALL		ANSWORK,EQ,1,my_sex_girl_papa_hune

	//下に進む
	_IFVAL_CALL		TMPWK0,EQ,0,case3_boy_papa_t101_r0101
