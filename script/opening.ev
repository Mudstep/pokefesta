	_BGM_SET		MUS_INTER_V,0

	_TALK_KEYWAIT	msg_mama_01_p06_t101_r0101
	_TALK_CLOSE

	//一歩引く
	_SET_OBJ_ANIME	PARAM1,mama_01_t101_r0201_anime01
	_OBJ_ANIME_WAIT

	//TVの前に行く
	_SET_OBJ_ANIME	EV_OBJ_SPID,hero_01_opening_anime61
	_OBJ_ANIME_WAIT

	_CALL			common_opening_tv_event_02

	//母の方を向く
	_SET_OBJ_ANIME	EV_OBJ_SPID,turn_l_anime
	_OBJ_ANIME_WAIT

	_TALK_KEYWAIT	msg_mama_01_p12_t101_r0101

	_TALK_KEYWAIT	msg_mama_01_p07_t101_r0101
