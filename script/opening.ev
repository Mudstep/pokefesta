	//主人公の方を向く
	_SET_OBJ_ANIME	TMPWK1,turn_hero_site_anime
	_OBJ_ANIME_WAIT

	_SE				SE_GYOE_MARK
	_SET_OBJ_ANIME	TMPWK1,gyoe_mark_anime
	_OBJ_ANIME_WAIT
	
	_SET_OBJ_ANIME	TMPWK1,gyoe_mark_wait_anime
	_OBJ_ANIME_WAIT

	_TIME_WAIT		20

	//主人公に近づいてくる
	_IFVAL_CALL		TMPWK0,EQ,0,case0_boy_papa_t101_r0101
	_IFVAL_CALL		TMPWK0,EQ,1,case0_girl_papa_t101_r0101

	_TALK_KEYWAIT	msg_senri_01_p01_t101_r0101

	_ITEM_EVENT		ITEM_HUNENOTIKETTO,1

	_TALK_KEYWAIT	msg_senri_01_p02_t101_r0101
	_TALK_CLOSE

	//出かける
	_IFVAL_CALL		TMPWK0,EQ,0,case1_boy_papa_t101_r0101
	_IFVAL_CALL		TMPWK0,EQ,1,case1_girl_papa_t101_r0101

	_TALK_KEYWAIT	msg_senri_01_p03_t101_r0101
	_TALK_CLOSE

	//出かける２
	_IFVAL_CALL		TMPWK0,EQ,0,case2_boy_papa_t101_r0101
	_IFVAL_CALL		TMPWK0,EQ,1,case2_girl_papa_t101_r0101

	_SE				SE_DOOR

	//センリを消す
	_OBJ_DELETE		TMPWK1

	_FLAG_SET		FE_HUNENOTIKETTO_GET
	_LDVAL			WK_SCENE_T101_R0101,4
	_LDVAL			WK_SCENE_T101_R0201,4
	_TALK_END
	_END

