/********************************************************************/
/*							ポス(ゴール)							*/
/********************************************************************/
SCRIPT_LABEL	ev_field_r110_pos01
SCRIPT_LABEL	ev_field_r110_pos02
	_TALK_START

	//主人公の方を向く
	_SET_OBJ_ANIME	CYCLEM1_03_FIELD_R110,turn_r_anime
	_OBJ_ANIME_WAIT

	_CALL		keisoku_obj_script
	_TALK_END
	_END

keisoku_obj_script:
	//タイムをゲットする
	_PROGRAM		GOAL_CYCLE_ROAD
	_TALK_KEYWAIT	msg_cyclem1_03_p01_field_r110

	//評価点により分岐
	_SWITCH			ANSWORK
	_CASEJUMP		10,case0_cycle_goal
	_CASEJUMP		9,case1_cycle_goal
	_CASEJUMP		8,case1_cycle_goal
	_CASEJUMP		7,case1_cycle_goal
	_CASEJUMP		6,case2_cycle_goal
	_CASEJUMP		5,case2_cycle_goal
	_CASEJUMP		4,case2_cycle_goal
	_CASEJUMP		3,case3_cycle_goal
	_CASEJUMP		2,case3_cycle_goal
	_CASEJUMP		1,case3_cycle_goal
	_CASEJUMP		0,case4_cycle_goal
	_END

//10
case0_cycle_goal:
	_TALK_KEYWAIT	msg_cyclem1_03_p02_field_r110
	_JUMP			common_cycle_end
	_END
//7-9
case1_cycle_goal:
	_TALK_KEYWAIT	msg_cyclem1_03_p03_field_r110
	_JUMP			common_cycle_end
	_END
//4-6
case2_cycle_goal:
	_TALK_KEYWAIT	msg_cyclem1_03_p04_field_r110
	_JUMP			common_cycle_end
	_END
//1-3
case3_cycle_goal:
	_TALK_KEYWAIT	msg_cyclem1_03_p05_field_r110
	_JUMP			common_cycle_end
	_END
//0
case4_cycle_goal:
	_TALK_KEYWAIT	msg_cyclem1_03_p06_field_r110
	_JUMP			common_cycle_end
	_END

//共通終了処理
common_cycle_end:
	//計測結果終了
	_LDVAL			WK_SCENE_CYCLE_ROAD,3

	_BGM_SPECIAL_CLR
	_BGM_NOW_SET
	_RET


/********************************************************************/
/*							サポート								*/
/********************************************************************/
SCRIPT_LABEL	ev_field_r110_pos03
	_LDVAL			TMPWK0,1
	_JUMP			ev_r110_poscommon
	_END

SCRIPT_LABEL	ev_field_r110_pos04
	_LDVAL			TMPWK0,2
	_JUMP			ev_r110_poscommon
	_END

SCRIPT_LABEL	ev_field_r110_pos05
	_LDVAL			TMPWK0,3
	_JUMP			ev_r110_poscommon
	_END

ev_r110_poscommon:	
	_TALK_START

	_GET_MY_SEX		//性別で曲を分ける
	_IFVAL_CALL		ANSWORK,EQ,0,mus_boy_01_field_r110	//男の子
	_IFVAL_CALL		ANSWORK,EQ,1,mus_girl_01_field_r110	//女の子

	//振り向く
	_SET_OBJ_ANIME	SUPPORT_01_FIELD_R110,turn_d_anime
	_OBJ_ANIME_WAIT

	//「！」
	_SET_OBJ_ANIME	SUPPORT_01_FIELD_R110,gyoe_mark_anime
	_OBJ_ANIME_WAIT

	//ウェイト
	_SET_OBJ_ANIME	SUPPORT_01_FIELD_R110,gyoe_mark_wait_anime
	_OBJ_ANIME_WAIT

	_TIME_WAIT		30

	//サポートキャラが近づいてくる:場所によりアニメ分岐
	_IFVAL_CALL	TMPWK0,EQ,1,ev_support_anime_p01_field_r110
	_IFVAL_CALL	TMPWK0,EQ,2,ev_support_anime_p02_field_r110
	_IFVAL_CALL	TMPWK0,EQ,3,ev_support_anime_p03_field_r110

	//男女の判別をする
	_GET_MY_SEX
	_IFVAL_JUMP		ANSWORK,EQ,0,boy_01_field_r110	//男の子
	_IFVAL_JUMP		ANSWORK,EQ,1,girl_01_field_r110	//女の子
	_TALK_END
    _END

//曲切り替え
mus_boy_01_field_r110:
	_BGM_SET	MUS_GIRL_SUP,1
	_RET

mus_girl_01_field_r110:
	_BGM_SET	MUS_BOY_SUP,1
	_RET

//男の子
boy_01_field_r110:
	_TALK_KEYWAIT   msg_heroine_01_p01_field_r110

	//相手のポケモンを変更する
	_SWITCH			WK_FIRST_POKE
	_CASEJUMP		0,case0_my_sex_boy_battle_field_r110
	_CASEJUMP		1,case1_my_sex_boy_battle_field_r110
	_CASEJUMP		2,case2_my_sex_boy_battle_field_r110
	_END

//トレーナー戦へ
//タイプ炎
case0_my_sex_boy_battle_field_r110:
	_TRAINER_EVENT_FIGHT	SUPPORTW02_02,msg_heroine_01_p02_field_r110
	_JUMP			common_heroine_battle_field_r110
	_END

//タイプ水
case1_my_sex_boy_battle_field_r110:
	_TRAINER_EVENT_FIGHT	SUPPORTW03_02,msg_heroine_01_p02_field_r110
	_JUMP			common_heroine_battle_field_r110
	_END

//タイプ草
case2_my_sex_boy_battle_field_r110:
	_TRAINER_EVENT_FIGHT	SUPPORTW01_02,msg_heroine_01_p02_field_r110
	_JUMP			common_heroine_battle_field_r110
	_END

//共通処理
common_heroine_battle_field_r110:
	_TALK_KEYWAIT   msg_heroine_01_p03_field_r110

	//共通アイテム取得処理
	_CALL			item_get_field_r110

	_TALK_KEYWAIT   msg_heroine_01_p04_field_r110

	//共通終了処理
	_JUMP			end_support_field_r110
    _END

//--------------------------------------------------------------------
//女の子
//--------------------------------------------------------------------
girl_01_field_r110:
	_TALK_KEYWAIT   msg_hero_01_p01_field_r110

	//相手のポケモンを変更する
	_SWITCH			WK_FIRST_POKE
	_CASEJUMP		0,case0_my_sex_girl_battle_field_r110
	_CASEJUMP		1,case1_my_sex_girl_battle_field_r110
	_CASEJUMP		2,case2_my_sex_girl_battle_field_r110
	_END

//トレーナー戦へ
//タイプ炎
case0_my_sex_girl_battle_field_r110:
	_TRAINER_EVENT_FIGHT	SUPPORTM02_02,msg_hero_01_p02_field_r110
	_JUMP			common_hero_battle_field_r110
	_END

//タイプ水
case1_my_sex_girl_battle_field_r110:
	_TRAINER_EVENT_FIGHT	SUPPORTM03_02,msg_hero_01_p02_field_r110
	_JUMP			common_hero_battle_field_r110
	_END

//タイプ草
case2_my_sex_girl_battle_field_r110:
	_TRAINER_EVENT_FIGHT	SUPPORTM01_02,msg_hero_01_p02_field_r110
	_JUMP			common_hero_battle_field_r110
	_END

//共通処理
common_hero_battle_field_r110:
	_TALK_KEYWAIT   msg_hero_01_p03_field_r110

	//共通アイテム取得処理
	_CALL			item_get_field_r110

	_TALK_KEYWAIT   msg_hero_01_p04_field_r110

	//共通終了処理
	_JUMP			end_support_field_r110
    _END

//共通アイテム取得処理
item_get_field_r110:
	_ITEM_EVENT		ITEM_DAUZINGUMASIN,1
	_RET

//共通終了処理
end_support_field_r110:
	_TALK_CLOSE

	//自転車に乗り換え
	_IFVAL_CALL		TMPWK0,EQ,1,ev_support_pos_p01_field_r110
	_IFVAL_CALL		TMPWK0,EQ,2,ev_support_pos_p02_field_r110
	_IFVAL_CALL		TMPWK0,EQ,3,ev_support_pos_p03_field_r110

	//右向きにする
	_SXY_SITE_CHANGE	SUPPORT_01_FIELD_R110,MV_RIGHT
	_SXY_SITE_CHANGE	SUPPORT_02_FIELD_R110,MV_RIGHT

	_OBJ_DELETE		SUPPORT_01_FIELD_R110
	_OBJ_APPEAR		SUPPORT_02_FIELD_R110

	_TIME_WAIT		45

	//サポートキャラが去っていく:場所によりアニメ分岐
