init_ctrl_field_r110:
	//前のマップがゲートだったか
	_PROGRAM		BEFORE_MAP_CYCLE
	_END

//--------------------------------------------------------------------
//						FLAG_CHANGE_LABEL
//--------------------------------------------------------------------
flag_change_field_r110:
	_CALL			set_evil_char
	_CALL			set_support_char
	_CALL			set_support_cycle_char

	//サイクリングロード計測時は曲を変更する
	_IFVAL_CALL		WK_SCENE_CYCLE_ROAD,EQ,1,ev_mus_chg_field_r110

	_END

//サイクリングロード計測時は曲を変更する
ev_mus_chg_field_r110:
	_BGM_SPECIAL_SET	MUS_CYCLING
	_RET

//--------------------------------------------------------------------
//						SP_SCRIPT_LABEL
//--------------------------------------------------------------------
scene_field_r110:
	//自転車Sに乗ってゲートを通過していたら
	SP_SCRIPT_DATA	WK_SCENE_CYCLE_ROAD,1,ev_scene_cycle_start_02
	SP_SCRIPT_END

ev_scene_cycle_start_02:
	_PROGRAM		START_CYCLE_ROAD
	_LDVAL			WK_SCENE_CYCLE_ROAD,2
	_RET


