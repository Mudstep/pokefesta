/*							ダイゴ(POS)								*/
/********************************************************************/
SCRIPT_LABEL	ev_field_r118_pos01
	_TALK_START
	_LDVAL		TMPWK0,0

	//向きを上へ(※ダート自転車対策）
	_SET_OBJ_ANIME	EV_OBJ_SPID,turn_u_anime
	_OBJ_ANIME_WAIT				//アニメ終了待ち

	//左へ移動
	_SET_OBJ_ANIME	DAIGO_01_FIELD_R118,daigo_01_field_r118_anime01
	_OBJ_ANIME_WAIT				//アニメ終了待ち

	_JUMP		ev_common_field_r118_pos01
	_END

SCRIPT_LABEL	ev_field_r118_pos02
	_TALK_START
	_LDVAL		TMPWK0,1

	//向きを上へ(※ダート自転車対策）
	_SET_OBJ_ANIME	EV_OBJ_SPID,turn_u_anime
	_OBJ_ANIME_WAIT				//アニメ終了待ち

	_JUMP		ev_common_field_r118_pos01
	_END

SCRIPT_LABEL	ev_field_r118_pos03
	_TALK_START
	_LDVAL		TMPWK0,2

	//向きを上へ(※ダート自転車対策）
	_SET_OBJ_ANIME	EV_OBJ_SPID,turn_u_anime
	_OBJ_ANIME_WAIT				//アニメ終了待ち

	//右へ移動
	_SET_OBJ_ANIME	DAIGO_01_FIELD_R118,daigo_03_field_r118_anime01
	_OBJ_ANIME_WAIT				//アニメ終了待ち

	_JUMP		ev_common_field_r118_pos01
	_END

//共通処理
ev_common_field_r118_pos01:
	_SE		SE_DANSA
	//進んでくる(go)
	_SET_OBJ_ANIME	DAIGO_01_FIELD_R118,daigo_01_field_r118_anime03
	_OBJ_ANIME_WAIT				//アニメ終了待ち

	_TIME_WAIT		30

	_TALK_KEYWAIT	msg_daigo_01_p01_field_r118
	_TALK_CLOSE

	//去っていく
	_IFVAL_CALL		TMPWK0,EQ,0,case0_daigo_01_field_r118
	_IFVAL_CALL		TMPWK0,EQ,1,case1_daigo_01_field_r118
	_IFVAL_CALL		TMPWK0,EQ,2,case2_daigo_01_field_r118

	_LDVAL			WK_SCENE_FIELD_R118,1
	_OBJ_DELETE		DAIGO_01_FIELD_R118
	_TALK_END
	_END

//去っていく
case0_daigo_01_field_r118:
	_SET_OBJ_ANIME	DAIGO_01_FIELD_R118,daigo_01_field_r118_anime02
	_OBJ_ANIME_WAIT				//アニメ終了待ち
	_RET

case1_daigo_01_field_r118:
	_SET_OBJ_ANIME	DAIGO_01_FIELD_R118,daigo_02_field_r118_anime02
	_OBJ_ANIME_WAIT				//アニメ終了待ち
	_RET

case2_daigo_01_field_r118:
	_SET_OBJ_ANIME	DAIGO_01_FIELD_R118,daigo_03_field_r118_anime02
	_OBJ_ANIME_WAIT				//アニメ終了待ち
	_RET

