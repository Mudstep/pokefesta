/********************************************************************/
/*							釣り人1									*/
/********************************************************************/
SCRIPT_LABEL	ev_fishing1_01_p01_field_r118
	_TALK_OBJ_START

	//既に貰っている
	_IF_FLAGON_JUMP	FE_IITURIZAO_GET,ev_fishing1_01_p05_field_r118

	//いいつりざおは　いいよ！
	_TALK_YES_NO	msg_fishing1_01_p01_field_r118
	_IFVAL_JUMP	ANSWORK,EQ,1,ev_fishing1_01_p02_field_r118
	_IFVAL_JUMP	ANSWORK,EQ,0,ev_fishing1_01_p04_field_r118
	_END

//「はい」
ev_fishing1_01_p02_field_r118:
	_TALK_KEYWAIT	msg_fishing1_01_p02_field_r118

	//「いいつりざお」ゲット
	_ITEM_EVENT		ITEM_IITURIZAO,1
	_FLAG_SET		FE_IITURIZAO_GET

	_TALK_KEYWAIT	msg_fishing1_01_p03_field_r118
	_TALK_OBJ_END
	_END

//「いいえ」
ev_fishing1_01_p04_field_r118:
	_TALK_KEYWAIT	msg_fishing1_01_p04_field_r118
	_TALK_OBJ_END
	_END

//「いいつりざお」入手後
ev_fishing1_01_p05_field_r118:
	_TALK_KEYWAIT	msg_fishing1_01_p05_field_r118
	_TALK_OBJ_END
	_END


