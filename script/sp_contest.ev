/********************************************************************/
/*						性別チェック								*/
/********************************************************************/
hero_sexcheck_sp_contest:
	_GET_MY_SEX
	_IFVAL_JUMP	ANSWORK,EQ,0,hero_boy_sp_contest	//男の子
	_IFVAL_JUMP	ANSWORK,EQ,1,hero_girl_sp_contest	//女の子
    _RET

//男の子
hero_boy_sp_contest:
	_LDVAL		OBJCHRWORK4,HERO	//主人公の男キャラを登録
    _RET

//女の子
hero_girl_sp_contest:
	_LDVAL		OBJCHRWORK4,HEROINE	//主人公の女キャラを登録
    _RET


