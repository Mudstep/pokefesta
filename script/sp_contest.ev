ev_hero_01_gohome_sp_contest:
	_SWITCH	WK_SCENE_SP_CONTEST03
	_CASEJUMP	1,ev_hero_01_t106_sp_contest//シダケに帰る
	_CASEJUMP	2,ev_hero_01_t105_sp_contest//ハジツゲに帰る
	_CASEJUMP	3,ev_hero_01_c102_sp_contest//カイナに帰る
	_CASEJUMP	4,ev_hero_01_c106_sp_contest//ミナモに帰る
	_CASEJUMP	5,ev_hero_01_tusin_sp_contest//ミナモ(通信)に帰る
	_RET


ev_hero_01_t106_sp_contest:
	_INC_RECORD	CNT_CONTEST	//伝説に出場回数プラス
	_MAP_CHANGE	T106,T106_R0101,5,4/*ワープ*/
	_STOP_SCRIPT
	_END

ev_hero_01_t105_sp_contest:
	_INC_RECORD	CNT_CONTEST	//伝説に出場回数プラス
	_MAP_CHANGE	T105,T105_R0101,5,4/*ワープ*/
