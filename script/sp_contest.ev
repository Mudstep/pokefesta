SCRIPT_LABEL	scene_contest_02_sp_contest
	_CALL		contest_script_after	//コンテスト呼び出し
	_CALL		ev_hero_01_gohome_sp_contest	//コンテストから帰る
	_END


ev_sp_contest_init:
	_OBJ_INVISIBLE	EV_OBJ_SPID,FIELD,FIELD_T101	//主人公を消す map,div適当
	_CALL	ev_kankyaku_dmy_sp_contest
	_END	

