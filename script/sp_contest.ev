/********************************************************************/
/*							一次審査の曲を鳴らす					*/
/********************************************************************/
flag_change_sp_contest:
	_CALL	ev_musicset_sp_contest
	_LDVAL	LOCALWORK0,1//オブジェキャラコードを配置するときに使用
	_CALL	ev_kankyaku_01_put_sp_contest
	_END

ev_musicset_sp_contest://一次審査の曲鳴らす
	_PROGRAM			SET_CON_MUS
	_IFVAL_CALL	ANSWORK,EQ,0,ev_musicset_00_sp_contest	//通信親機１
	_IFVAL_CALL	ANSWORK,EQ,1,ev_musicset_01_sp_contest	//通信子機１
	_IFVAL_CALL	ANSWORK,EQ,2,ev_musicset_02_sp_contest	//通信子機２
	_IFVAL_CALL	ANSWORK,EQ,3,ev_musicset_03_sp_contest	//通信子機３
	_IFVAL_CALL	ANSWORK,EQ,4,ev_musicset_04_sp_contest	//一人プレイ
	_RET

ev_musicset_00_sp_contest:
	_BGM_SPECIAL_SET	MUS_TEST1
	_RET

ev_musicset_01_sp_contest:
	_BGM_SPECIAL_SET	MUS_TEST2
	_RET

ev_musicset_02_sp_contest:
	_BGM_SPECIAL_SET	MUS_TEST3
	_RET

ev_musicset_03_sp_contest:
	_BGM_SPECIAL_SET	MUS_TEST4
	_RET

ev_musicset_04_sp_contest:
	_BGM_SPECIAL_SET	MUS_CONTEST0
	_RET


ev_dmyhaiti_sp_contest:
	_IFVAL_CALL		LOCALWORK9,EQ,1,chg_contest
	_END

chg_contest:
	_CALL	ev_kankyaku_dmy_sp_contest	//ダミーＯＢＪを配置呼び出し
	_RET

/********************************************************************/
/*		WK_SCENE_SP_CONTESTに1が入るとコンテストが開始される。		*/
/*		WK_SCENE_SP_CONTESTに0が入るとコンテストが終了される。		*/
/********************************************************************/
scene_contest_check_sp_contest:
	SP_SCRIPT_DATA	WK_SCENE_SP_CONTEST,1,scene_contest_02_sp_contest
	SP_SCRIPT_END

/********************************************************************/
/*			主人公のＯＢＪの表示を消す処理の呼び出し				*/
/********************************************************************/
obj_sp_contest:
	OBJ_CTRL_DATA	WK_SCENE_SP_CONTEST,1,ev_sp_contest_init
	OBJ_CTRL_END

/********************************************************************/
/*						ポケモンコンテスト							*/
/*							■シーン１■							*/
