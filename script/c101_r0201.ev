	_CASEJUMP		5,ev_papa_01_p05_c101_r0201 //バッチ３つ
	_CASEJUMP		6,ev_gymleader_05_02		//バッチ４つ
	_CASEJUMP		7,already_papa_batch_get	//バッチをゲットした

	_TALK_KEYWAIT	msg_gymleader_05_01
	_TALK_CLOSE

	_SWITCH			RG_SITE
	_CASEJUMP		SITE_D,site_u_to_hero_c101_r0201			//上から話し掛け
	_CASEJUMP		SITE_U,site_d_to_hero_c101_r0201			//下から話し掛け
