SCRIPT_LABEL	ev_gymleader_05_01
	_TALK_OBJ_START

	_SWITCH			WK_SCENE_C101_R0201
	_CASEJUMP		2,ev_papa_01_p02_c101_r0201	//ミツルが訪れるイベント終了
	_CASEJUMP		3,ev_papa_01_p03_c101_r0201	//バッチ１つ
