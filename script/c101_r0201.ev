/********************************************************************/
SCRIPT_LABEL	ev_gymleader_05_02
	_TALK_KEYWAIT	msg_gymleader_05_10

	//戦闘へ
	_TRAINER_EVENT_FIGHT	LEADERM_05,msg_gymleader_05_11

	_TALKMSG		msg_gymleader_05_12
	_TALK_WAIT
	_CALL			leader_jingle_set
	_TALK_KEYWAIT	msg_gymleader_05_13

	_FLAG_SET		FT_GYM_05_LEADER
	_LDVAL			WK_SCENE_C101_R0201,7
	_FLAG_SET		SYS_BATCH05_GET

	//ハギ削除
	_CALL			ev_hagi_del

	//キンセツジムリーダー移動
	_FLAG_SET		FV_TETTSUSEN_01_C103_R0101
	_FLAG_RESET		FV_TETTSUSEN_01_FIELD_C103

	//ジム内のトレーナーフラグ全てONにする
	_LDVAL			TMPWK0,5
	_CALL			gym_trainer_all_flagon

	//流行部屋に技マシンくれるOBJ表示する
	_FLAG_RESET		FV_FREAK1_01_T103_R0301

	//ジムのトラップを解除
	_CALL			ev_bg_change_get_gym05_batch
	_PROGRAM		MAKE_MAP_BG

	_JUMP			gymleader_05_wazam_get
	_END

