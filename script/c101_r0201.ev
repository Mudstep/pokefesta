//共通処理
ev_common_gymleader_05_p01:
	_OBJ_APPEAR		MITSURU_01_C101_R0201
	_SE				SE_DOOR

	//ドアの方を向く
	_IFVAL_CALL		TMPWK0,EQ,0,case0_gym_anime01
	_IFVAL_CALL		TMPWK0,EQ,1,case1_gym_anime01
	_IFVAL_CALL		TMPWK0,EQ,2,case2_gym_anime01
	_IFVAL_CALL		TMPWK0,EQ,3,case3_gym_anime01

	_TALK_KEYWAIT	msg_mitsuru_01_p01_c101_r0201

	_TALK_KEYWAIT	msg_gymleader_05_02

	_TALK_KEYWAIT	msg_mitsuru_01_p02_c101_r0201

	_TALK_KEYWAIT	msg_gymleader_05_03

	//主人公と父が向き合う
	_IFVAL_CALL		TMPWK0,EQ,0,case0_gym_anime03
	_IFVAL_CALL		TMPWK0,EQ,1,case1_gym_anime03
	_IFVAL_CALL		TMPWK0,EQ,2,case2_gym_anime03
	_IFVAL_CALL		TMPWK0,EQ,3,case3_gym_anime03

	_TALK_KEYWAIT	msg_gymleader_05_04

	//父がミツルの方を向く
	_IFVAL_CALL		TMPWK0,EQ,0,case0_gym_anime04
	_IFVAL_CALL		TMPWK0,EQ,1,case1_gym_anime04
	_IFVAL_CALL		TMPWK0,EQ,2,case2_gym_anime04
	_IFVAL_CALL		TMPWK0,EQ,3,case3_gym_anime04

	_TALK_KEYWAIT	msg_gymleader_05_18

	//効果音は指摘が合ったらつけることになった
	_TALK_KEYWAIT	msg_mitsuru_01_p06_c101_r0201

	_TALK_KEYWAIT	msg_mitsuru_01_p03_c101_r0201

	_IFVAL_CALL		TMPWK0,EQ,0,case0_gym_anime10
	_IFVAL_CALL		TMPWK0,EQ,1,case1_gym_anime10
	_IFVAL_CALL		TMPWK0,EQ,2,case2_gym_anime10
	_IFVAL_CALL		TMPWK0,EQ,3,case3_gym_anime10

	//主人公の方を向く(下からの話し掛けがdefault_site_animeでうまくいかない？ので)
	_IFVAL_CALL		TMPWK0,EQ,0,case0_gym_anime05
	_IFVAL_CALL		TMPWK0,EQ,1,case1_gym_anime05
	_IFVAL_CALL		TMPWK0,EQ,2,case2_gym_anime05
	_IFVAL_CALL		TMPWK0,EQ,3,case3_gym_anime05

	_TALK_KEYWAIT	msg_mitsuru_01_p04_c101_r0201
	_TALK_CLOSE

	//マップ遷移のタイミングでのBGM変更を無効にする
	_FLAG_SET		CTRL_BGM_CHANGE
	_BGM_SET		MUS_TSURETEK,0	

	//捕獲しに行く
	_IFVAL_CALL		TMPWK0,EQ,0,case0_gym_anime02
	_IFVAL_CALL		TMPWK0,EQ,1,case1_gym_anime02
	_IFVAL_CALL		TMPWK0,EQ,2,case2_gym_anime02
	_IFVAL_CALL		TMPWK0,EQ,3,case3_gym_anime02

	//OBJ消す
	_OBJ_DELETE		MITSURU_01_C101_R0201

	//ミツル母を消しておく
	_FLAG_SET		FV_WOMAN2_01_FIELD_C101

	//ミツル訪れるイベント終了
	_LDVAL			WK_SCENE_C101_R0201,1

	//ミツルとの捕獲イベント開始
	_LDVAL			WK_SCENE_FIELD_C101,2
	_FLAG_RESET		FV_MITSURU_01_FIELD_C101

	//ミツルを出す
	_FLAG_RESET		FV_MITSURU_01_C101_R0201

	//研究所のサポートを消す
	_FLAG_SET		FV_SUPPORT_01_T101_R0301

	//評価システム開始
	_PROGRAM		HYOUKA_INIT

	_MAP_CHANGE		FIELD,FIELD_C101,15,8
	_STOP_SCRIPT

	_TALK_OBJ_END
	_END

