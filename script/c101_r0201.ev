//--------------------------------------------------------------------
//					バッチをゲットしている
//--------------------------------------------------------------------
already_papa_batch_get:
	//「なぞの実」チェック
	_CALL			ev_ram_kinomi_chk
	_IFVAL_JUMP		ANSWORK,EQ,TRUE,ev_ram_kinomi_get

	//技マシン貰っているか
	_IF_FLAGOFF_JUMP	FE_WAZAM_01_C101_R0201,gymleader_05_wazam_get

	//ゲームクリアしているか
	_IF_FLAGON_JUMP	SYS_GAME_CLEAR,game_clear_gymleader_05

	_TALK_KEYWAIT	msg_gymleader_05_17
	_TALK_OBJ_END
	_END

//--------------------------------------------------------------------
//							RAM木の実
//--------------------------------------------------------------------
//貰えるかチェック
ev_ram_kinomi_chk:
	//「なぞの実」存在するか
	_FUNC_CALL		ANSWORK,CHK_RAM_SEED
	_IFVAL_JUMP		ANSWORK,EQ,FALSE,ev_ram_kinomi_end

	//バッグ、パソコンに「なぞの実」ないか
	_CHECK_ITEM		ITEM_NAZONOMI,1
	_IFVAL_JUMP		ANSWORK,EQ,TRUE,ev_ram_kinomi_end
	_CHECK_BOX_ITEM	ITEM_NAZONOMI,1
	_IFVAL_JUMP		ANSWORK,EQ,TRUE,ev_ram_kinomi_end

	//許可ワークが１でなかったら
	_IFVAL_JUMP		WK_RAM_SEED_GET,EQ,FALSE,ev_ram_kinomi_end

	//RAM木の実貰える条件そろっている
	_TALK_KEYWAIT	msg_gymleader_05_kinomi
	_LDVAL			ANSWORK,TRUE
	_RET

//貰えない
ev_ram_kinomi_end:
	_LDVAL			ANSWORK,FALSE
	_RET

//取得処理
ev_ram_kinomi_get:
	_ITEM_EVENT		ITEM_NAZONOMI,1
	_IF_ZERO_JUMP	ev_fail_item

	//許可ワークを０にクリアする
	_LDVAL			WK_RAM_SEED_GET,0

	_TALK_OBJ_END
	_END

