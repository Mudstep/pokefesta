//--------------------------------------------------------------------
//							BG_CHANGE_LABEL
//--------------------------------------------------------------------
bg_change_c101_r0201:
	//バッチを４つもっている
	_IFVAL_JUMP		WK_SCENE_C101_R0201,EQ,6,ev_bg_change_start_gym05
	//父からバッチをもらった
	_IFVAL_CALL		WK_SCENE_C101_R0201,GE,7,ev_bg_change_get_gym05_batch
	_END

//バッチを４つもっている
ev_bg_change_start_gym05:
	_LDVAL			PARAM1,1	//セル書き換えウェイトなし
	_CALL			gym05_cell_change_ptn01	//始めの部屋の扉は開ける

	//その部屋のトレーナーを倒しているか見て対応しているptnを呼ぶ
	_IF_TR_FLAGON_CALL	ELITEM_01,gym05_cell_change_ptn02
	_IF_TR_FLAGON_CALL	ELITEW_01,gym05_cell_change_ptn03
	_IF_TR_FLAGON_CALL	ELITEM_02,gym05_cell_change_ptn04
	_IF_TR_FLAGON_CALL	ELITEW_02,gym05_cell_change_ptn05
	_IF_TR_FLAGON_CALL	ELITEM_03,gym05_cell_change_ptn06
	_IF_TR_FLAGON_CALL	ELITEW_03,gym05_cell_change_ptn07
	_IF_TR_FLAGON_CALL	ELITEM_04,gym05_cell_change_ptn08
	_END

//父からバッチをもらった
ev_bg_change_get_gym05_batch:
	_LDVAL			PARAM1,1	//セル書き換えウェイトなし
	_CALL			gym05_cell_change_ptn01
	_CALL			gym05_cell_change_ptn02
	_CALL			gym05_cell_change_ptn03
	_CALL			gym05_cell_change_ptn04
	_CALL			gym05_cell_change_ptn05
	_CALL			gym05_cell_change_ptn06
	_CALL			gym05_cell_change_ptn07
	_CALL			gym05_cell_change_ptn08
	_RET


