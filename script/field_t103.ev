/********************************************************************/
/*							流行入力キャラ							*/
/********************************************************************/
SCRIPT_LABEL	ev_boy1_01_p01_field_t103
	_TALK_OBJ_START

	_CALL			set_popword_buf0

	_TALK_YES_NO	msg_boy1_01_p01_field_t103
	_IFVAL_JUMP		ANSWORK,EQ,1,yes_boy1_01_p02_field_t103	//「はい」
	_IFVAL_JUMP		ANSWORK,EQ,0,no_boy1_01_p02_field_t103	//「いいえ」
	_END

//--------------------------------------------------------------------
//はやってるだろ？「はい」
//--------------------------------------------------------------------
yes_boy1_01_p02_field_t103:
	_TALK_KEYWAIT	msg_boy1_01_p06_field_t103
	_TALK_OBJ_END
	_END

//--------------------------------------------------------------------
//はやってるだろ？「いいえ」
//--------------------------------------------------------------------
no_boy1_01_p02_field_t103:
	_TALK_KEYWAIT	msg_boy1_01_p02_field_t103

	//簡易会話へ(＠１に新しい流行語が入る)
	_LDVAL			PARAM0,KAIWA_MODE_POPULAR
	_CALL			ev_kaiwa_input_start
	_TALK_OBJ_RECOVER
	_IFVAL_JUMP		ANSWORK,EQ,1,hayari_change	//キャンセルしてない
	_IFVAL_JUMP		ANSWORK,EQ,0,hayari_cancel	//キャンセル
	_END

//--------------------------------------------------------------------
//流行を入れ替えた
//--------------------------------------------------------------------
hayari_change:
	//記録カウントアップ
	_INC_RECORD		CNT_TREND

	//流行を入れかえれなかったか
	_IFVAL_JUMP	PARAM0,EQ,0,hayari_no_change

	_TALK_KEYWAIT	msg_boy1_01_p03_field_t103

	_TALK_OBJ_END
	_END

//--------------------------------------------------------------------
//簡易会話画面でキャンセルした
//--------------------------------------------------------------------
hayari_cancel:
	_TALK_KEYWAIT	msg_boy1_01_p05_field_t103
	_TALK_OBJ_END
	_END
	
//--------------------------------------------------------------------
//流行を入れかえられなかった場合
//--------------------------------------------------------------------
hayari_no_change:
	_TALK_KEYWAIT	msg_boy1_01_p04_field_t103
	_TALK_OBJ_END
	_END
	
//--------------------------------------------------------------------
//本日すでに流行を変更した
//--------------------------------------------------------------------
flagon_hayari:
	_TALK_KEYWAIT	msg_boy1_01_p07_field_t103
	_TALK_OBJ_END
	_END


