/********************************************************************/
//							ハギ老人
/********************************************************************/
SCRIPT_LABEL		hagi_script_field_t103
	_TALK_OBJ_START

	//手紙イベントクリアしてなかったら
	_IF_FLAGOFF_JUMP	FE_TEGAMI_CLEAR,ev_hagi_stoper_field_t103

	//どこへ行きたいのかね？
	_TALKMSG	msg_oldman1_01_p06_field_t103
	_TALK_WAIT

	_SELECT_WINDOW_CSR	21,6,HAGI_MENU01,2,0	//B有効

	_SWITCH	ANSWORK
	_CASEJUMP		0,case0_hagi_01_p01_field_t103
	_CASEJUMP		1,case1_hagi_01_p01_field_t103
	_CASEJUMP		2,case2_hagi_01_p01_field_t103
	_CASECANCEL		case2_hagi_01_p01_field_t103
	_END

//トウカ
case0_hagi_01_p01_field_t103:
	_TALK_KEYWAIT	msg_oldman1_01_p07_field_t103
	_TALK_CLOSE

	_JUMP			ev_fune_01_p01_field_t103
	_TALK_OBJ_END
	_END

//カイナ
case1_hagi_01_p01_field_t103:
	_TALK_KEYWAIT	msg_oldman1_01_p08_field_t103
	_TALK_CLOSE

	_JUMP			ev_fune_02_p01_field_t103
	_TALK_OBJ_END
	_END

//やめる
case2_hagi_01_p01_field_t103:
	_TALK_KEYWAIT	msg_oldman1_01_p09_field_t103
	_TALK_CLOSE
	_TALK_OBJ_END
	_END

//手紙イベントクリアしてなかったら
ev_hagi_stoper_field_t103:
	_TALK_YES_NO	msg_oldman1_01_p02_field_t103
	_IFVAL_JUMP		ANSWORK,EQ,1,yes_hagi_01_p01_field_t103	//「はい」

	_TALK_KEYWAIT	msg_oldman1_01_p04_field_t103
	_TALK_OBJ_END
	_END

//カナズミに戻る
yes_hagi_01_p01_field_t103:
	_TALK_KEYWAIT	msg_oldman1_01_p03_field_t103
	_TALK_CLOSE
	_JUMP			ev_fune_01_p01_field_t103
	_END


