EVENT_DATA_START	field_c102_event_data
FLAG_CHANGE_LABEL	flag_change_field_c102
EVENT_DATA_END

//--------------------------------------------------------------------
//						FLAG_CHANGE_LABEL
//--------------------------------------------------------------------
flag_change_field_c102:
//藍バージョンの場合の処理を記述
#if ( PM_VERSION == VERSION_SAPPHIRE )

	//敵の団をセットする
	_LDVAL			OBJCHRWORK1,AQUALF1
	_LDVAL			OBJCHRWORK2,AQUALM1

//朱バージョンの場合の処理を記述
#elif ( PM_VERSION == VERSION_RUBY )

	//敵の団をセットする
	_LDVAL			OBJCHRWORK1,MAGMAF1
	_LDVAL			OBJCHRWORK2,MAGMAM1

#endif

	//博物館の入場料払ったをクリアする
	_LDVAL			WK_SCENE_C102_R0501,0

	_CALL			ev_arrive_flag_set_c102

	//クスノキ艦長が帰ってきていてインタビュー受けている
	_IFVAL_CALL		WK_SCENE_FIELD_C102,EQ,1,obj_chg_field_c102

	_END

ev_arrive_flag_set_c102:
	_FLAG_SET		SYS_C102_ARRIVE				//到着フラグセット
	_LDVAL			WK_SCENE_SP_CONTEST,0		//コンテストしてない
	_FLAG_SET		FV_INTERVM1_01_C102_R0201	//インタビュアーを消す
	_RET

obj_chg_field_c102:
	_SXY_POS_CHANGE		KUSUNOKI_01_FIELD_C102,28,13

	//やじうまに行く
	_SXY_POS_CHANGE		OLDWOMAN2_01_FIELD_C102,25,13
	_SXY_POS_CHANGE		BOY4_01_FIELD_C102,25,14
	_SXY_POS_CHANGE		COOK1_01_FIELD_C102,27,16
	_SXY_POS_CHANGE		GIRL1_01_FIELD_C102,28,16
	_SXY_POS_CHANGE		BIGMAN1_01_FIELD_C102,29,16
	_SXY_POS_CHANGE		MIDDLEM1_01_FIELD_C102,31,14

	//移動制限を変更する
	_SXY_SITE_CHANGE	KUSUNOKI_01_FIELD_C102,MV_DOWN

	_SXY_SITE_CHANGE	OLDWOMAN2_01_FIELD_C102,MV_DR
	_SXY_SITE_CHANGE	BOY4_01_FIELD_C102,MV_UR
	_SXY_SITE_CHANGE	COOK1_01_FIELD_C102,MV_UP
	_SXY_SITE_CHANGE	GIRL1_01_FIELD_C102,MV_UP
	_SXY_SITE_CHANGE	BIGMAN1_01_FIELD_C102,MV_UP
	_SXY_SITE_CHANGE	MIDDLEM1_01_FIELD_C102,MV_LEFT

	_RET


