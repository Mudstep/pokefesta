	_TALK_START

	_TALK_KEYWAIT	msg_studym1_01_p01_field_c102

	_TALK_KEYWAIT	msg_intervw1_01_p02_field_c102
	_TALK_CLOSE

	//インタビュアーはカメラマンの方を向く
	_SET_OBJ_ANIME	INTERVW1_01_FIELD_C102,turn_r_anime
	_OBJ_ANIME_WAIT 			//アニメ終了待ち

	_TIME_WAIT		10

	//カメラマンはインタビュアーの方を向く
	_SET_OBJ_ANIME	CAMERA1_01_FIELD_C102,turn_d_anime
	_OBJ_ANIME_WAIT 			//アニメ終了待ち

	_TIME_WAIT		25

	//カメラマン去る
	_SET_OBJ_ANIME	KUSUNOKI_01_FIELD_C102,kusunoki_01_field_c102_anime03
	_SET_OBJ_ANIME	EV_OBJ_SPID,hero_01_field_c102_anime02
	_SET_OBJ_ANIME	INTERVW1_01_FIELD_C102,intervw1_01_field_c102_anime01
	_SET_OBJ_ANIME	CAMERA1_01_FIELD_C102,camera_01_field_c102_anime01
	_OBJ_ANIME_WAIT 			//アニメ終了待ち

	//OBJ消す
	_OBJ_DELETE		INTERVW1_01_FIELD_C102
	_OBJ_DELETE		CAMERA1_01_FIELD_C102

	_TALK_KEYWAIT	msg_studym1_01_p02_field_c102

	//振り向く
	_SET_OBJ_ANIME	KUSUNOKI_01_FIELD_C102,turn_u_anime
	_OBJ_ANIME_WAIT 			//アニメ終了待ち

	_TALK_KEYWAIT	msg_studym1_01_p03_field_c102

#if ( PM_VERSION == VERSION_SAPPHIRE )
	_BGM_SET		MUS_AQA_0,0
#elif ( PM_VERSION == VERSION_RUBY )
	_BGM_SET		MUS_MGM0,0
#endif

	_TALK_KEYWAIT	msg_aquaboss_01_p01_field_c102

	//きょろきょろさせる
	_SET_OBJ_ANIME	COOK1_01_FIELD_C102,turn_l_anime
	_SET_OBJ_ANIME	BIGMAN1_01_FIELD_C102,turn_l_anime
	_SET_OBJ_ANIME	OLDWOMAN2_01_FIELD_C102,obj_01_field_c102_anime01
	_SET_OBJ_ANIME	BOY4_01_FIELD_C102,hatena_mark_anime
	_SET_OBJ_ANIME	MIDDLEM1_01_FIELD_C102,obj_01_field_c102_anime02
	_OBJ_ANIME_WAIT 			//アニメ終了待ち

	//下を向く
	_SET_OBJ_ANIME	KUSUNOKI_01_FIELD_C102,turn_d_anime
	_OBJ_ANIME_WAIT 			//アニメ終了待ち

	_TALK_KEYWAIT	msg_studym1_01_p04_field_c102

	//クスノキ「！」
	_SE		SE_GYOE_MARK
	_SET_OBJ_ANIME	KUSUNOKI_01_FIELD_C102,gyoe_mark_anime
	_OBJ_ANIME_WAIT 			//アニメ終了待ち

	//クスノキウェイト
	_SET_OBJ_ANIME	KUSUNOKI_01_FIELD_C102,gyoe_mark_wait_anime
	_OBJ_ANIME_WAIT 			//アニメ終了待ち

	_TALK_KEYWAIT	msg_studym1_01_p05_field_c102

	_SET_OBJ_ANIME	KUSUNOKI_01_FIELD_C102,turn_hero_site_anime
	_OBJ_ANIME_WAIT 			//アニメ終了待ち

	_TALK_KEYWAIT	msg_studym1_01_p06_field_c102
	_TALK_CLOSE

	//部屋内に移動する
	_SET_OBJ_ANIME	KUSUNOKI_01_FIELD_C102,kusunoki_01_field_c102_anime02
	_SET_OBJ_ANIME	EV_OBJ_SPID,hero_01_field_c102_anime01
	_OBJ_ANIME_WAIT 			//アニメ終了待ち

	//OBJ消す
	_OBJ_DELETE		KUSUNOKI_01_FIELD_C102

	_FLAG_RESET		FV_KUSUNOKI_01_C102_R0601
	_FLAG_RESET		FV_SUBMARINE1_01_C102_R0601
	_FLAG_RESET		FV_EVILM1_01_C102_R0601
	_FLAG_RESET		FV_AQUABOSS_01_C102_R0601

	//ギャラリーを移動させる
	_LDVAL			WK_SCENE_FIELD_C102,2

	_MAP_CHANGE		C102,C102_R0601,11,14
	_STOP_SCRIPT

	_TALK_END
	_END

