/********************************************************************/
/*							がんばり屋								*/
/********************************************************************/
SCRIPT_LABEL	ev_trainerw1_02_p01_field_c102
	_TALK_OBJ_START

	//手持ちの先頭のポケモン名取得
	_GET_TEMOTI_MONSNAME	0
	_TALK_KEYWAIT	msg_trainerw1_02_p01_field_c102

	//手持ちの先頭のポケモンが「がんばリボン」を持っているか
	_FUNC_CALL		ANSWORK,CHK_GANBA_RIBBON
	_IFVAL_CALL		ANSWORK,EQ,TRUE,already_get_ganba_ribbon_field_c102

	//努力値の合計を取得する
	_FUNC_CALL		ANSWORK,CHK_GANBA_RIBBON_PARA_EXP
	_IFVAL_CALL		ANSWORK,EQ,FALSE,no_trainerw1_02_p01_field_c102

	_TALK_KEYWAIT	msg_trainerw1_02_p02_field_c102
	
	_JINGLE			MUS_FANFA4
	_TALKMSG		msg_trainerw1_02_p03_field_c102
	_JINGLE_WAIT

	_TALK_KEYWAIT	msg_trainerw1_02_p04_field_c102

	_PROGRAM		SET_GANBA_RIBBON
	_TALK_OBJ_END
	_END

//努力値がMAXではない
no_trainerw1_02_p01_field_c102:
	_TALK_KEYWAIT	msg_trainerw1_02_p05_field_c102
	_TALK_OBJ_END
	_END

