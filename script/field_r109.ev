/********************************************************************/
/*						１０９番道路での船乗り込み					*/
/********************************************************************/
SCRIPT_LABEL	ev_fune_01_p01_field_r109
	//クリアする
	_CALL			ev_fune_pos_clear

	//船の上にOBJがくるようにプライオリティを操作する
	_OBJ_PRI_CHG	OLDMAN1_01_FIELD_R109,FIELD,FIELD_R109,0
	_OBJ_PRI_CHG	EV_OBJ_SPID,FIELD,FIELD_R109,0

	//ハギ乗り込みアニメ
	_SET_OBJ_ANIME	OLDMAN1_01_FIELD_R109,oldman1_01_field_r109_anime01
	_OBJ_ANIME_WAIT
	_OBJ_DELETE		OLDMAN1_01_FIELD_R109

	//現状は外に行くアニメ
	_SWITCH			RG_SITE
	_CASEJUMP		SITE_D,site_u_poke1_01_p01_field_r109	//上から話し掛け
	_CASEJUMP		SITE_L,site_r_poke1_01_p01_field_r109	//右から話し掛け
	_CASEJUMP		SITE_R,site_l_poke1_01_p01_field_r109	//左から話し掛け
	_END

site_u_poke1_01_p01_field_r109:	//上からの話
	_SET_OBJ_ANIME	EV_OBJ_SPID,hero_01_field_r109_anime01
	_OBJ_ANIME_WAIT
	_JUMP			ev_fune_01_p02_field_r109
	_END

site_l_poke1_01_p01_field_r109:	//左から話し掛け
	_SET_OBJ_ANIME	EV_OBJ_SPID,hero_01_field_r109_anime03
	_OBJ_ANIME_WAIT
	_JUMP			ev_fune_01_p02_field_r109
	_END

site_r_poke1_01_p01_field_r109:	//右から話し掛け
	_SET_OBJ_ANIME	EV_OBJ_SPID,hero_01_field_r109_anime04
	_OBJ_ANIME_WAIT
	_JUMP			ev_fune_01_p02_field_r109
	_END

SCRIPT_LABEL	ev_fune_01_p02_field_r109
	//主人公を見えなくする
	_OBJ_INVISIBLE	EV_OBJ_SPID,FIELD,FIELD_R109	

	_CALL			set_hagi_fune_bgm

	//船で移動する
	_SET_OBJ_ANIME	FUNE_01_FIELD_R109,fune_01_field_r109_anime01
	_SET_OBJ_ANIME	EV_OBJ_SPID,fune_01_field_r109_anime01
	_OBJ_ANIME_WAIT

	_CALL			reset_hagi_fune_bgm

	//主人公を見えるようにして降りる
	_OBJ_VISIBLE	EV_OBJ_SPID,FIELD,FIELD_T103
	_SET_OBJ_ANIME	EV_OBJ_SPID,hero_01_field_r109_anime02
	_OBJ_ANIME_WAIT

	//ムロタウン
	_OBJ_APPEAR		FUNE_01_FIELD_T103			//船を出す
	_FLAG_RESET		FV_FUNE_01_FIELD_T103		//一応

	_SXY_POS_CHANGE	OLDMAN1_01_FIELD_T103,12,8	//ハギ老人の座標移動
	_OBJ_APPEAR		OLDMAN1_01_FIELD_T103		//ハギ老人を出す

	//船の上にOBJがくるようにプライオリティを操作する
	_OBJ_PRI_CHG	OLDMAN1_01_FIELD_T103,FIELD,FIELD_T103,0

	//ハギ降りる
	_SET_OBJ_ANIME	OLDMAN1_01_FIELD_T103,oldman1_01_field_r109_anime02
	_OBJ_ANIME_WAIT

	_FLAG_RESET		FV_HAGI_01_FIELD_T103		//一応

	//ロード１０９
	_FLAG_SET		FV_FUNE_01_FIELD_R109		//船を消す
	_OBJ_INVISIBLE	FUNE_01_FIELD_R109,FIELD,FIELD_R109

	_TALK_KEYWAIT	msg_oldman1_01_p05_field_t103
	_TALK_CLOSE

	_LDWK			WK_SCENE_FUNE_POS,TMPWK0	//元に戻す

	//プライオリティを元に戻す
	_OBJ_PRI_OFF	EV_OBJ_SPID,FIELD,FIELD_R109
	_OBJ_PRI_OFF	OLDMAN1_01_FIELD_T103,FIELD,FIELD_T103

	//sxy情報を上書きする
	_SXY_POS_OVERWRITE	OLDMAN1_01_FIELD_T103

	_TALK_OBJ_END
	_END

