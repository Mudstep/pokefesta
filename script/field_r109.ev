/********************************************************************/
/*							ハギ老人								*/
/********************************************************************/
SCRIPT_LABEL	hagi_script_field_r109
	_TALK_OBJ_START
	//デボンの荷物を返していない
	_IF_FLAGOFF_JUMP	FE_AQUA_01_C102_R0502,no_clear_nimotsu_oldman1_01_field_r109

	//通常時
	_JUMP			normal_talk_oldman1_01_p01_field_r109
	_END

//荷物イベントをクリアしていなかったら
no_clear_nimotsu_oldman1_01_field_r109:
	_TALKMSG		msg_oldman1_01_p02_field_r109

	_TALK_YES_NO	msg_oldman1_01_p02_field_r109
	_IFVAL_JUMP		ANSWORK,EQ,FALSE,cancel_oldman1_01_p01_field_r109
	_JUMP			go_oldman1_01_p01_field_r109
	_END

//通常時
normal_talk_oldman1_01_p01_field_r109:
	//ムロタウンに行きたいか
	_TALKMSG		msg_oldman1_01_p06_field_r109
	_TALK_WAIT

	_SELECT_WINDOW_CSR	21,8,HAGI_MENU02,1,0	//B有効

	_SWITCH	ANSWORK
	_CASEJUMP		0,go_oldman1_01_p01_field_r109
	_CASEJUMP		1,normal_cancel_oldman1_01_p01_field_r109
	_CASECANCEL		normal_cancel_oldman1_01_p01_field_r109
	_END

//ムロタウンに行く
go_oldman1_01_p01_field_r109:
	_TALK_KEYWAIT	msg_oldman1_01_p03_field_r109
	_TALK_CLOSE
	_JUMP			ev_fune_01_p01_field_r109
	_END

//ムロタウンに行かない
cancel_oldman1_01_p01_field_r109:
	_TALK_KEYWAIT	msg_oldman1_01_p04_field_r109
	_TALK_OBJ_END
	_END

//ムロタウンに行かない(通常)
normal_cancel_oldman1_01_p01_field_r109:
	_TALK_KEYWAIT	msg_oldman1_01_p07_field_r109
	_TALK_OBJ_END
	_END


