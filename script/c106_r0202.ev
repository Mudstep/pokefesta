//							スクリプト本体
//--------------------------------------------------------------------
/********************************************************************/
/*							館長									*/
/********************************************************************/
SCRIPT_LABEL	ev_gentlem1_01_p04_c106_r0202//絵の枚数チェック！
	_TALK_START
	
	_IF_FLAGON_JUMP	FE_GOODS_GET_C106_R0202,ev_gentlem1_01_p17_c106_r0202
	
	_FUNC_CALL	PARAM0,	ART_COUNT_CHK
	_SWITCH	PARAM0
	_CASEJUMP	1,ev_gentlem1_01_p05_c106_r0202
	_CASEJUMP	2,ev_gentlem1_01_p05_c106_r0202
	_CASEJUMP	3,ev_gentlem1_01_p05_c106_r0202
	_CASEJUMP	4,ev_gentlem1_01_p05_c106_r0202
	_CASEJUMP	5,ev_gentlem1_01_p06_c106_r0202
	_EASY_OBJ_MSG	msg_gentlem1_01_p04_c106_r0202	
	_END
	
ev_gentlem1_01_p05_c106_r0202://１〜４枚
	_EASY_OBJ_MSG	msg_gentlem1_01_p05_c106_r0202
	_END


ev_gentlem1_01_p06_c106_r0202://五枚そろった
	_SET_OBJ_ANIME	GENTLEM1_01_C106_R0202,turn_hero_site_anime
	_OBJ_ANIME_WAIT
	_TALK_KEYWAIT	msg_gentlem1_01_p06_c106_r0202
	_SET_OBJ_ANIME	GENTLEM1_01_C106_R0202,musueum_c106_r0202_anime02
	_TALK_KEYWAIT	msg_gentlem1_01_p07_c106_r0202
	_JUMP	ev_gentlem1_01_p16_c106_r0202
	_END
	
ev_gentlem1_01_p16_c106_r0202://グッズをもらう
	_SET_OBJ_ANIME	GENTLEM1_01_C106_R0202,turn_hero_site_anime
	_TALK_KEYWAIT	msg_gentlem1_01_p13_c106_r0202
	_GOODS_EVENT	GOODS_GARASUNOOBUJE
	_IFVAL_JUMP	ANSWORK,EQ,0,ev_gentlem1_01_p15_c106_r0202
	_FLAG_SET	FE_GOODS_GET_C106_R0202
	_TALK_CLOSE
	_TALK_END
	_END

ev_gentlem1_01_p15_c106_r0202://グッズがいっぱいのとき
	_CALL				ev_fail_goods_ret	//失敗メッセージ
	_TALK_KEYWAIT	msg_gentlem1_01_p15_c106_r0202
	_TALK_CLOSE
	_TALK_END
	_END

ev_gentlem1_01_p17_c106_r0202://グッズを貰っている
	_EASY_OBJ_MSG	msg_gentlem1_01_p17_c106_r0202
	_TALK_END
	_END

