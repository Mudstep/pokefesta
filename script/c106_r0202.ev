/********************************************************************/
/*								scene0								*/
/*							館長のお話								*/
/********************************************************************/
ev_gentlem1_01_p01_c106_r0202:
	_TALK_START
	_SET_OBJ_ANIME	GENTLEM1_01_C106_R0202,turn_hero_site_anime
	_SET_OBJ_ANIME	EV_OBJ_SPID,musueum_c106_r0202_anime01
	_OBJ_ANIME_WAIT
	_EASY_MSG	msg_gentlem1_01_p01_c106_r0202
	_SET_OBJ_ANIME	GENTLEM1_01_C106_R0202,musueum_c106_r0202_anime02
	_OBJ_ANIME_WAIT
	_SET_OBJ_ANIME	EV_OBJ_SPID,musueum_c106_r0202_anime02
	_OBJ_ANIME_WAIT
	_EASY_MSG	msg_gentlem1_01_p02_c106_r0202
	_SET_OBJ_ANIME	GENTLEM1_01_C106_R0202,turn_hero_site_anime
	_OBJ_ANIME_WAIT
	_SET_OBJ_ANIME	EV_OBJ_SPID,musueum_c106_r0202_anime01
	_OBJ_ANIME_WAIT
	_EASY_MSG	msg_gentlem1_01_p03_c106_r0202
	_LDWK	WK_SCENE_C106_R0202,1
	_TALK_END
	_END

//-------------------------------------------------------------------
//					　　scene0のアニメ
//--------------------------------------------------------------------
musueum_c106_r0202_anime01:
	.byte	AC_WALK_TURN_L
	.byte	ACMD_END

musueum_c106_r0202_anime02:
	.byte	AC_SITE_U
	.byte	AC_WAIT_16
	.byte	ACMD_END

